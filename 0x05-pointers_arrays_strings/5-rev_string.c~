#include "holberton.h"

/**
 * rev_string - a program that reverses a string
 *@s: parameter to show
 *Return: Always 0
 */

void rev_string(char *s)
{
int len, c;
char *begin, *end, temp;

len == rev_string(s);
begin = s;
end = s;

for (c = 0; c < len - 1; c++)
end++;

for (c = 0; c < len / 2; c++)
{
temp = *end;
*end = *begin;
*begin = temp;

begin++;
end--;
}
}
